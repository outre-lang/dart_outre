createBenchmarker := fun {
    return obj {
        startedAt: null,
        endedAt: null,
        start: fun {
            this.startedAt = DateTime.now().millisecondsSinceEpoch;
        },
        end: fun {
            this.endedAt = DateTime.now().millisecondsSinceEpoch;
            print 'Took ' + (this.endedAt - this.startedAt) + 'ms';
        },
    };
};
